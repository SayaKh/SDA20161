package addStd;

/**
 *
 * @author Ghada
 */
public class Student {
    private String id;
    private String name;
    private String sp;

    public Student(String id, String name, String sp) {
        this.id = id;
        this.name = name;
        this.sp = sp;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getSp() {
        return sp;
    }

    public void setSp(String sp) {
        this.sp = sp;
    }

    @Override
    public String toString() {
        return "Student {" + "id = " + id + " , name = " + name + " , sp = " + sp + '}'+"\n";
    }
    
    
}
